// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum EnumAbc {
  A
  B
  C
}

enum EnumDef {
  D
  E
  F
}

enum EnumGhi {
  G
  H
  I
}

model Abc {
  id    BigInt
  enum  EnumAbc
  enum2 EnumDef
  enum3 EnumGhi
  float Float

  @@id([id, enum, enum2])
}
